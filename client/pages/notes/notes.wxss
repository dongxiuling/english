/* pages/notes/notes.wxss */
/* 笔记 */
.note_list{
  background: #eeeeee;
}
.note{
  padding: 0 25rpx;
  margin:20rpx 0;
  background: #ffffff;
}
.cont{
  width: 700rpx;
  height: 500rpx;
}
.tab_list{
  background: #ffffff;
  overflow: hidden;
}
.tab{
  width: 120rpx;
  height: 70rpx;
  float: right;
}
.tab text{
  font-size: 30rpx;
  color: #848484;
}
/* 图标字体 */
@font-face {
  font-family: 'iconfont';  /* project id 441255 */
  src: url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.eot');
  src: url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.woff') format('woff'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

