/* pages/articles/articles.wxss */
/* 页面 */
page{
  background: #f2f2f2;
}
/* 文章 */
.art_list{
  background: #f2f2f2;
}
.art{
  display: flex;
  flex-direction: column;
  margin-top:10rpx;
  background: #ffffff;
}
.cont{
  padding:10rpx 30rpx;
  font-size:34rpx;
  color:#666666;
}
.img image{
  width:200rpx;
  height:200rpx;
  margin-right:10rpx;
}
.tab_list{
  background: #ffffff;
  overflow: hidden;
}
.time{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #d0d0d0;
  margin-left:30rpx;  
  float: left; 
}
.tab{
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  float: right;
}
.tab text{
  font-size: 30rpx;
  color: #848484; 
}
.tab .gray{
  font-size: 30rpx;
  color: #848484; 
}
.tab .green{
  font-size: 30rpx;
  color: #209E85; 
}
/* 图标字体 */
@font-face {
  font-family: 'iconfont';  /* project id 441255 */
  src: url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.eot');
  src: url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.woff') format('woff'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_441255_i8r3a74p581l9pb9.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-msnui-share:before { content: "\e677"; }

.icon-pinglun:before { content: "\e7d2"; }

.icon-fanhui:before { content: "\e610"; }

.icon-dianzan:before { content: "\e668"; }