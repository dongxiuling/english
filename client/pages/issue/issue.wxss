.icon-photo:before { content: "\e62b"; }

.icon-write:before { content: "\e608"; }



/* 开示 */
.issue-head{
  width: 90%;
  margin:20rpx 0 0 5%;
  position: relative;
  border: 1px solid #dddddd;
}

.head{
  display: flex;
  justify-content: space-between;
}
/* 标题 */

.icon-write{
  display: flex;
  margin: 0 4px;
  color:  #dddddd;
}
.content{
  width: 100%;
  height:260rpx;
 padding:20rpx 20rpx;
 box-sizing: border-box;
  margin-top:10rpx;
  font-size: 13px;
 
}
.icon-photo{
  color: #737373;
  font-size: 30px;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  margin-bottom:23rpx;
  margin-left: 20rpx;
  border: 1px solid  #dddddd;
  
}
.issue-pub{
  height: 87rpx;
   background:#209E85;
   font-family: Regular;
   font-size: 16px; 
   color:#fff;
  text-align: center;
  line-height: 87rpx;
  width: 100%;
  position:fixed;
  bottom:0;
 
}
.photo{
  width: 90%;
  height: 300rpx;
  margin: 0 40rpx;
}