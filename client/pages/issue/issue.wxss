.icon-write:before { content: "\e608"; }

.icon-back:before { content: "\e669"; }


/* 开示 */
.issue-head{
  width: 90%;
  margin:10px 0 0 5%;
}
.back{
  width: 40px;
  height: 30px;
  margin-top:5px;
  display: flex;
  align-items: center;
  border: 1px solid #d0d0d0;
  text-align: center;
  border-radius: 10rpx;
  font-size: 12px;
    
}
.head{
  display: flex;
  justify-content: space-between;
}
.head text{
  font-weight: 900;
  width: 40px;
  height:30px;
  font-size: 12px;
  margin: 5px 2px 5px 0;
  text-align: center;
  line-height: 30px;
   border: 1px solid #d0d0d0; 
  border-radius: 10rpx;  
}

/* 标题 */
.issue-title{
  margin-top:20px;
  width: 99%;
 display: flex;
  align-items: center;
 font-size: 16px;
 border: 1px solid #d0d0d0;
  border-radius: 10rpx;  
}
.icon-write{
  display: flex;
  margin: 0 4px;
  color: #d0d0d0;
}
.content{
  width: 99%;
  height:150px;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;  
   margin-top:10px;
}